<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#66BB6A</item>
        <item name="colorPrimaryDark">#4CAF50</item>
        <item name="colorAccent">#FFC107</item>
        <item name="colorSecondary">#FFC107</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:fontFamily">@font/workb</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="ToolBarStyle" parent="Theme.AppCompat">
        <item name="android:textColorPrimary">@android:color/white</item>
        <item name="android:textColorSecondary">@android:color/white</item>
        <item name="actionMenuTextColor">@android:color/white</item>
        <item name="android:background">#66BB6A</item>

    </style>

    <style name="autocompletar">
        <item name="android:fontFamily">@font/wh</item>
    </style>

    <style name="botonPrimario">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:background">#66BB6A</item>
        <item name="android:fontFamily">@font/wb</item>
    </style>

    <style name="botonSecundario">
        <item name="android:fontFamily">@font/wb</item>
        <item name="android:textColor">@android:color/white</item>
    </style>

    <style name="textoPrimarioTitulo">
        <item name="android:textColor">#4CAF50</item>
        <item name="android:fontFamily">@font/rb</item>
    </style>

    <style name="textoPrimario">
        <item name="android:textColor">#4CAF50</item>
    </style>

    <style name="nav">
        <item name="android:background">#4CAF50</item>
    </style>

    <style name="textoSecundario">
        <item name="android:textColor">@android:color/white</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>
